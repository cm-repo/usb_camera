<?xml version="1.0"?>
<package>
  <name>usb_camera</name>
  <version>0.0.0</version>
  <description>The usb_camera package</description>
  <maintainer email="quchao@seas.upenn.edu">chao</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

